{
  "defaultSeverity": "error",
  "extends": [
    "tslint-plugin-prettier",
    "tslint-config-prettier",
    "tslint-react",
    "tslint-config-airbnb-base",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "function-name": [
      true,
      {
        "function-regex": "[wd]+$"
      }
    ],
    "import-name": false,
    "max-line-length": false,
    "ter-max-len": false,
    "ter-indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "variable-name": false
  },
  "rulesDirectory": []
}
